    <body cz-shortcut-listen="true">
        
        <div class="content"> 
            <form id="reg" action="" method="POST" class="form-horizontal" role="form">
                <fieldset>
			        <legend>Login Information</legend>
                    <div class= "form-group">
                        <label for="username">Username</label>
                        <div class="input-group col-sm-4">
                            <span class="input-group-addon"><i class="fa fa-user fa-fw"></i></span>
                            <input id="username" class="form-control" type="text" name="username" required="" placeholder="Enter Username">
                        </div>
                    </div>
            
                    <div class= "form-group">
                        <label for="password">Password</label>
                        <div class="input-group col-sm-4">
                            <span class="input-group-addon"><i class="fa fa-user fa-fw"></i></span>
                            <input id="password" class="form-control" type="password" name="password" placeholder="Enter Password" required="">
                        </div>
                        <span class="help-block"><strong>Note: Passwords are encrypted and cannot be retreived. Please do not forget your password!</strong></span>
                    </div>
                
                    <div class= "form-group" style="margin-top: -1%">
                        <label for="password2">Confirm Password</label>
                        <div class="input-group col-sm-4">
                            <span class="input-group-addon"><i class="fa fa-key fa-fw"></i></span>
                            <input id="password2" class="form-control" type="password" name="password2" placeholder="Confirm your Password" required="">
                        </div>
                    </div>
               </fieldset>
                <input id="signUp" class="btn btn-default" type="submit" value="Submit">
            </form>
        </div> 
        
		
